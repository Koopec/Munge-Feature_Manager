{
  "name": "munge-feature-manager",
  "displayName": "Munge feature manager",
  "description": "extension munge",
  "publisher": "us",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["workspaceContains:**/*.java"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "munge-feature-manager.compileWithMunge",
        "title": "Compile with Munge"
      },
      {
        "command": "munge-feature-manager.compileAllWithMunge",
        "title": "Compile all with Munge"
      },
      {
        "command": "munge-feature-manager.createVisualization",
        "title": "Create visualization"
      },
      {
        "command": "munge-feature-manager.createMinConfig",
        "title": "Create minimal config"
      },
      {
        "command": "munge-feature-manager.createDirStruct",
        "title": "Create directory structure"
      }
    ],
    "keybindings": [
      {
        "command": "munge-feature-manager.compileWithMunge",
        "key": "ctrl+shift+m"
      },
      {
        "command": "munge-feature-manager.createVisualization",
        "key": "ctrl+shift+d"
      },
      {
        "command": "munge-feature-manager.createMinConfig",
        "key": "ctrl+shift+q"
      },
      {
        "command": "munge-feature-manager.createDirStruct",
        "key": "ctrl+shift+u"
      },
      {
        "command": "munge-feature-manager.compileAllWithMunge",
        "key": "ctrl+shift+a"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1"
  },
  "dependencies": {
    "xml2js": "^0.6.2",
    "viz.js": "^2.1.2"
  }
}
